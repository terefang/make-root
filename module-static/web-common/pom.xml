    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>1.8</version>
                <inherited>false</inherited>
                <dependencies>
                    <dependency>
                        <groupId>org.redline-rpm</groupId>
                        <artifactId>redline</artifactId>
                        <version>1.2.9</version>
                        <classifier>jar-with-dependencies</classifier>
                    </dependency>
                    <dependency>
                        <groupId>org.apache.ant</groupId>
                        <artifactId>ant-compress</artifactId>
                        <version>1.5</version>
                    </dependency>
                    <dependency>
                        <groupId>ant-contrib</groupId>
                        <artifactId>ant-contrib</artifactId>
                        <version>1.0b3</version>
                    </dependency>
                    <!-- https://mvnrepository.com/artifact/org.apache.ant/ant-apache-bsf -->
                    <dependency>
                        <groupId>org.apache.ant</groupId>
                        <artifactId>ant-apache-bsf</artifactId>
                        <version>1.10.12</version>
                    </dependency>

                    <!-- https://mvnrepository.com/artifact/org.beanshell/bsh -->
                    <dependency>
                        <groupId>org.beanshell</groupId>
                        <artifactId>bsh</artifactId>
                        <version>2.0b5</version>
                    </dependency>
                    <!-- https://mvnrepository.com/artifact/commons-logging/commons-logging -->
                    <dependency>
                        <groupId>commons-logging</groupId>
                        <artifactId>commons-logging</artifactId>
                        <version>1.2</version>
                    </dependency>
                </dependencies>
                <executions>
                    <execution>
                        <id>ant</id>
                        <phase>package</phase>
                        <goals>
                            <goal>run</goal>
                        </goals>
                        <configuration>
                            <target xmlns:if="ant:if" xmlns:unless="ant:unless">
                                <taskdef classname="org.redline_rpm.ant.RedlineTask" name="xrpm" />
                                <taskdef classname="org.apache.ant.compress.taskdefs.Cpio" name="cpio" />
                                <taskdef resource="net/sf/antcontrib/antlib.xml"/>

                                <mkdir dir="${rpm.path}"/>
                                <mkdir dir="${project.build.directory}/${project.artifactId}"/>
                                <mkdir dir="${project.basedir}/src/main/dist"/>

                                <condition property="binary.exists">
                                    <resourceexists>
                                        <file file="${project.basedir}/src/main/dist/${prg.tag}"/>
                                    </resourceexists>
                                </condition>

                                <mkdir dir="${root.dir}/var/www/common/fonts"/>
                                <mkdir dir="${root.dir}/var/www/common/icons"/>
                                <mkdir dir="${root.dir}/var/www/common/html"/>
                                <mkdir dir="${root.dir}/var/www/common/lib"/>

                                <get src="https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip" dest="${project.basedir}/src/main/dist/chosen_v1.8.7.zip" />
                                <mkdir dir="${root.dir}/var/${project.artifactId}/chosen"/>
                                <exec dir="${root.dir}/var/${project.artifactId}/chosen" executable="unzip">
                                    <arg line="-o ${project.basedir}/src/main/dist/chosen_v1.8.7.zip" />
                                </exec>

                                <mkdir dir="${root.dir}/var/www/common/lib/chosen"/>
                                <copy todir="${root.dir}/var/www/common/lib/chosen">
                                    <fileset dir="${root.dir}/var/${project.artifactId}/chosen" includes="*.css"/>
                                    <fileset dir="${root.dir}/var/${project.artifactId}/chosen" includes="*.js"/>
                                </copy>

                                <get src="https://github.com/twbs/icons/releases/download/v1.11.2/bootstrap-icons-1.11.2.zip" dest="${project.basedir}/src/main/dist/bootstrap-icons-1.11.2.zip" />
                                <mkdir dir="${root.dir}/var/${project.artifactId}/bootstrap-icons"/>
                                <exec dir="${root.dir}/var/${project.artifactId}/bootstrap-icons" executable="unzip">
                                    <arg line="-o ${project.basedir}/src/main/dist/bootstrap-icons-1.11.2.zip" />
                                </exec>

                                <mkdir dir="${root.dir}/var/www/common/icons/bootstrap"/>
                                <copy todir="${root.dir}/var/www/common/icons/bootstrap">
                                    <fileset dir="${root.dir}/var/${project.artifactId}/bootstrap-icons/bootstrap-icons-1.11.2/font" includes="**/*.css"/>
                                    <fileset dir="${root.dir}/var/${project.artifactId}/bootstrap-icons/bootstrap-icons-1.11.2/font" includes="**/*.woff"/>
                                    <fileset dir="${root.dir}/var/${project.artifactId}/bootstrap-icons/bootstrap-icons-1.11.2/font" includes="**/*.woff2"/>
                                </copy>


                                <get src="https://github.com/shutterstock/rickshaw/archive/refs/tags/v1.7.0.zip" dest="${project.basedir}/src/main/dist/rickshaw-1.7.0.zip" />
                                <mkdir dir="${root.dir}/var/${project.artifactId}/rickshaw"/>
                                <exec dir="${root.dir}/var/${project.artifactId}/rickshaw" executable="unzip">
                                    <arg line="-o ${project.basedir}/src/main/dist/rickshaw-1.7.0.zip" />
                                </exec>

                                <mkdir dir="${root.dir}/var/www/common/lib/rickshaw"/>
                                <copy todir="${root.dir}/var/www/common/lib/rickshaw">
                                    <fileset dir="${root.dir}/var/${project.artifactId}/rickshaw/rickshaw-1.7.0" includes="*.css"/>
                                    <fileset dir="${root.dir}/var/${project.artifactId}/rickshaw/rickshaw-1.7.0" includes="*.js"/>
                                    <fileset dir="${root.dir}/var/${project.artifactId}/rickshaw/rickshaw-1.7.0/vendor" includes="*.js"/>
                                </copy>

                                <get src="https://github.com/coreui/coreui/releases/download/v4.3.2/coreui-4.3.2-dist.zip" dest="${project.basedir}/src/main/dist/coreui-4.3.2-dist.zip" />
                                <mkdir dir="${root.dir}/var/${project.artifactId}/coreui"/>
                                <exec dir="${root.dir}/var/${project.artifactId}/coreui" executable="unzip">
                                    <arg line="-o ${project.basedir}/src/main/dist/coreui-4.3.2-dist.zip" />
                                </exec>

                                <mkdir dir="${root.dir}/var/www/common/lib/coreui"/>
                                <copy todir="${root.dir}/var/www/common/lib/coreui">
                                    <fileset dir="${root.dir}/var/${project.artifactId}/coreui/coreui-4.3.2-dist"/>
                                </copy>

                                <get src="https://github.com/coreui/coreui/releases/download/v5.1.2/coreui-5.1.2-dist.zip" dest="${project.basedir}/src/main/dist/coreui-5.1.2-dist.zip" />
                                <mkdir dir="${root.dir}/var/${project.artifactId}/coreui5"/>
                                <exec dir="${root.dir}/var/${project.artifactId}/coreui5" executable="unzip">
                                    <arg line="-o ${project.basedir}/src/main/dist/coreui-5.1.2-dist.zip" />
                                </exec>

                                <mkdir dir="${root.dir}/var/www/common/lib/coreui5"/>
                                <copy todir="${root.dir}/var/www/common/lib/coreui5">
                                    <fileset dir="${root.dir}/var/${project.artifactId}/coreui5/coreui-5.1.2-dist"/>
                                </copy>


                                <for list="${css.urls}" delimiter=" &#10;" trim="true" param="ncss">
                                    <sequential>
                                        <script manager="bsf" language="beanshell"><![CDATA[
                                            part = "@{ncss}".split(",");
                                            project.setProperty("nurl",part[0]);
                                            project.setProperty("npath",part[1]);
                                            if(part.length==3)
                                            {
                                                project.setProperty("nfile",part[1]+"/"+part[2]);
                                            }
                                            else
                                            {
                                                project.setProperty("nfile",part[1]+"/"+part[0].substring(part[0].lastIndexOf('/')+1));
                                            }
                                        ]]></script>
                                        <mkdir dir="${root.dir}/var/www/common/lib/${npath}"/>
                                        <get src="${nurl}" dest="${root.dir}/var/www/common/lib/${nfile}" />
                                    </sequential>
                                </for>

                                <copy todir="${root.dir}/var/www/common/html">
                                    <fileset dir="src/main/dist/html"/>
                                </copy>
                                <copy todir="${root.dir}/var/www/common/fonts">
                                    <fileset dir="src/main/dist/fonts"/>
                                </copy>
                                <mkdir dir="${root.dir}/var/www/common/icons/fips"/>
                                <copy todir="${root.dir}/var/www/common/icons/fips">
                                    <fileset dir="src/main/dist/fips"/>
                                </copy>

                                <tar destfile="${project.build.directory}/${project.artifactId}.tar">
                                    <tarfileset prefix="common" dir="${root.dir}/var/www/common" />
                                </tar>

                                <xrpm
                                        architecture="X86_64"
                                        destination="${project.build.directory}"
                                        license="MIT"
                                        group="${project.artifactId}"
                                        name="toolbox-${project.artifactId}"
                                        summary="${project.artifactId}"
                                        distribution="${project.artifactId}"
                                        packager="${project.artifactId}"
                                        url=""
                                        vendor="${project.artifactId}"
                                        epoch="${buildNumber}"
                                        release="${buildNumber}"
                                        version="${timestamp}"
                                >
                                    <rpmfileset prefix="/var/www/common" filemode='444' dir="${root.dir}/var/www/common" username="root" group="root" addParents="false" />
                                </xrpm>

                            </target>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>

</project>